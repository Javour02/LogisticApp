<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OfertarActivo">

    <TextView
        android:id="@+id/textView23"
        android:layout_width="278dp"
        android:layout_height="29dp"
        android:layout_marginTop="56dp"
        android:textColor="#101010"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.195"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        tools:text="Disponible para el usuario" />

    <LinearLayout
        android:id="@+id/linearTransporte"
        android:layout_width="360dp"
        android:layout_height="352dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/añadirOferta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView23"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp"
            android:orientation="horizontal">

            <EditText
                android:id="@+id/largoDisp"
                android:layout_width="76dp"
                android:layout_height="61dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:hint="Largo"
                android:inputType="textPersonName"
                app:layout_constraintEnd_toEndOf="parent"
                tools:layout_editor_absoluteY="172dp" />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="20dp"
                android:text="x"
                app:layout_constraintEnd_toStartOf="@+id/largoDisp"
                tools:layout_editor_absoluteY="189dp" />

            <EditText
                android:id="@+id/altoDisp"
                android:layout_width="76dp"
                android:layout_height="61dp"
                android:layout_marginEnd="24dp"
                android:ems="10"
                android:hint="Alto"
                android:inputType="textPersonName"
                app:layout_constraintEnd_toStartOf="@+id/textView21"
                tools:layout_editor_absoluteY="172dp" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="32dp"
                android:text="x"
                app:layout_constraintEnd_toStartOf="@+id/altoDisp"
                tools:layout_editor_absoluteY="189dp" />

            <EditText
                android:id="@+id/anchoDis"
                android:layout_width="76dp"
                android:layout_height="61dp"
                android:ems="10"
                android:hint="Ancho"
                android:inputType="textPersonName"
                app:layout_constraintEnd_toStartOf="@+id/textView19"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="172dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp"
            android:orientation="horizontal">

            <EditText
                android:id="@+id/LugarSalida"
                android:layout_width="150dp"
                android:layout_height="59dp"
                android:ems="10"
                android:hint="Lugar de salida"
                android:inputType="textPersonName"
                app:layout_constraintEnd_toStartOf="@+id/textView22"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="199dp" />

            <TextView
                android:id="@+id/lSalida"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="->"
                app:layout_constraintEnd_toStartOf="@+id/LugarLlegada"
                tools:layout_editor_absoluteY="213dp" />

            <EditText
                android:id="@+id/LugarLlegada"
                android:layout_width="150dp"
                android:layout_height="58dp"
                android:layout_marginEnd="28dp"
                android:ems="10"
                android:hint="Lugar de Llegada"
                android:inputType="textPersonName"
                app:layout_constraintEnd_toEndOf="parent"
                tools:layout_editor_absoluteY="200dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <EditText
                android:id="@+id/horaSalida"
                android:layout_width="150dp"
                android:layout_height="59dp"
                android:ems="10"
                android:hint="Hora de salida"
                android:inputType="textPersonName"
                app:layout_constraintEnd_toStartOf="@+id/textView22"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="199dp" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:text="->"
                app:layout_constraintEnd_toStartOf="@+id/LugarLlegada"
                tools:layout_editor_absoluteY="213dp" />

            <EditText
                android:id="@+id/horaLlegada"
                android:layout_width="150dp"
                android:layout_height="58dp"
                android:layout_marginEnd="28dp"
                android:ems="10"
                android:hint="Hora de Llegada"
                android:inputType="textPersonName"
                app:layout_constraintEnd_toEndOf="parent"
                tools:layout_editor_absoluteY="200dp" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearAlmacenamiento"
        android:layout_width="360dp"
        android:layout_height="352dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/añadirOferta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView23"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView24"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="50dp"
                android:text="Espacio en Piso"
                app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName6"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="230dp" />

            <EditText
                android:id="@+id/piso"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_marginEnd="48dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Name"
                app:layout_constraintEnd_toEndOf="parent"
                tools:layout_editor_absoluteY="222dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="30dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView25"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="50dp"
                android:text="Espacio en altura"
                app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName6"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="230dp" />

            <EditText
                android:id="@+id/altura"
                android:layout_width="wrap_content"
                android:layout_height="48dp"
                android:layout_marginEnd="48dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Name"
                app:layout_constraintEnd_toEndOf="parent"
                tools:layout_editor_absoluteY="222dp" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linearOtros"
        android:layout_width="360dp"
        android:layout_height="352dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/añadirOferta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView23"
        android:padding="50dp">

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="50dp"
            android:text="Pieza completa"
            android:textColor="#040303"
            android:textSize="20dp"
            app:layout_constraintEnd_toStartOf="@+id/editTextTextPersonName6"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="230dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="Ultimos Detalles:"
        android:textColor="#401E1E"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/añadirOferta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="64dp"
        android:text="Añadir Oferta"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>